{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "store:default",
    "shell:allow-open",
    "stronghold:default",
    "os:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://**" }
      ]
    }
  ]
}